#include <iostream>

using namespace std;

int main(){
    
    int n1 = 0;
    int n2 = 0;
    int n3 = 0;
    
    cout << "digite o numero:";
    cin >> n1;
    cout << "digite o numero:";
    cin >> n2;
    cout << "digite o primeiro numero:";
    cin >> n3;

    cout << n1;
    cout << n2;
    cout << n3;

    if (n1 < n2 and n1 < n3){
        cout << n1;
        if (n2 < n3){
            cout << n2;
            cout << n3;
        }
        else{
            cout << n3;
            cout << n2;
        }
    }
    else if (n2 < n1 and n2 < n3){
        cout << n2;
        if (n1 < n3){
            cout << n1;
            cout << n3;
        }
        else{
            cout << n3;
            cout << n1;
        }
    }
    else if (n3 < n1 and n3 < n2){
        cout << n3;
        if(n1 < n2){
            cout << n1;
            cout << n2;
        }
        else{
            cout << n2;
            cout << n1;
        }
    }

    
    return 0;

}
