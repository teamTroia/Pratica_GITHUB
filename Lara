#include <iostream>

using namespace std;

int main(){
    int x, y, z;
    cin >> x;
    cin >> y;
    cin >> z;
    if (x < y and x < z and y < z){
        cout << x << " " << y << " " << z;
    }
    else if (y < x and y < z and x < z){
        cout << y << " " << x << " " << z;
    }
    else if (z < x and y > z and x < y){
        cout << z << " " << x << " " << y;
    }
    else if (z < x and y > z and x > y){
        cout << z << " " << y << " " << x;
    }
    else if (x < y and x < z and y > z){
        cout << x << " " << z << " " << y;
    }
    else if (y < x and y < z and x > z){
        cout << y << " " << z << " " << x;
    }
    return 0;
}
