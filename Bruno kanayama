Exercicio 2
#include <iostream> 
using namespace std;

int main(){
    int n1;

    cout << "Digite um numero de 1 a 9" << endl;
    cin >> n1;
    
    if (n1<0 and n1>10){
        cout << "Digite um numero de 1 a 9";
    }else{
        for (int i = 0; i <= 10; i++){
        cout << n1 << "x" << i << "=" << n1*i << endl;
        }
    }
    return 0;
}
