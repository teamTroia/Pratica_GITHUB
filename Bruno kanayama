#include <iostream> 
using namespace std;

int main(){
    int n1, n2, n3;
    
    cout << "Digite 3 numeros";
    cin >> n1;
    cin >> n2;
    cin >> n3;
    
    if (n1<n2 and n1<n3){
        cout<< n1;  
        if (n2<n3){
            cout<< n2,n3;
        }else if (n3<n2){
            cout<< (n3, n2);
        }
    }else if (n2<n1 and n2>n3){
        cout<< (n2);
        if (n1<n3){
            cout<< (n1,n3);
        }else if (n3<n1){
            cout<< (n3,n1);
        }
    }else if (n3<n2 and n3<n1){
        cout<<(n3);
        if (n2<n1){
            cout<< (n2, n1);
        }else if (n1<n2){
            cout<< (n2, n1);
        }   
    }
}  
